
using namespace std;
int main()
{
	const int size = 10;
	int x[size];

	for (int i = 0; i < size; i++)
	{
		cin >> x[i];
	}

	cout << "the even numbers " << endl;
	for (int i = 0; i < size; i++)
	{
		if (x[i] % 2 == 0)
		{
			cout << x[i] << " " << endl;
		}
	}

	cout << "the odd numbers " << endl;
		for (int i = 0; i < size; i++)
		{
			if (x[i] % 2 == 1)
			{
				cout << x[i] << " " << endl;
			}
		}
		return 0;
}
